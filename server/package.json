{
  "name": "create-nodejs-express-app",
  "version": "1.7.0",
  "description": "Create a Node.js app for building production-ready RESTful APIs using Express, by running one command",
  "bin": "bin/createNodejsApp.js",
  "main": "src/index.js",
  "repository": "https://github.com/hagopj13/node-express-boilerplate.git",
  "author": "Hagop Jamkojian <hagopj13@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "pm2 start ecosystem.api.config.json --no-daemon",
    "start:job": "pm2 start ecosystem.job.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js --inspect",
    "dev:job": "cross-env NODE_ENV=development nodemon src/jobs/index.js --inspect",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "vercel-build": "prisma generate",
    "import-locale": "cross-env NODE_ENV=development node src/locale/import-locales.js"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.772.0",
    "@databases/pg-test": "^3.1.2",
    "@prisma/client": "^6.8.2",
    "@smithy/node-http-handler": "^4.0.6",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.53.2",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "googleapis": "^148.0.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "ioredis": "^5.6.1",
    "ioredis-mock": "^8.9.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime": "^2.6.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.47",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.3.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^6.0.8",
    "prisma": "^6.8.2",
    "protobufjs": "^7.4.0",
    "qs": "^6.14.0",
    "sharp": "^0.33.5",
    "tough-cookie": "^4.1.4",
    "validator": "^13.0.0",
    "vnpay": "^2.2.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.5.3",
    "jest": "^29.7.0",
    "lint-staged": "^11.0.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^3.1.9",
    "prettier": "^2.0.5",
    "supertest": "^6.0.1"
  }
}
